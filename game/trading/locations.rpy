    class Location:
        def __init__(self, name, economic_power, dependencies=None):
            self.name = name
            self.economic_power = economic_power
            self.item_demands = {}
            self.dependencies = dependencies if dependencies else []

        def set_item_demand(self, item, demand):
            self.item_demands[item] = demand

        def update_economic_power(self, amount):
            self.economic_power = min(100, max(0, self.economic_power + amount))
            for dependency in self.dependencies:
                dependency.economic_power = (dependency.economic_power + self.economic_power) / 2

        def update_item_demand(self, item, amount):
            if item in self.item_demands:
                self.item_demands[item] = min(100, max(0, self.item_demands[item] + amount))
                for dependency in self.dependencies:
                    if item in dependency.item_demands:
                        dependency.item_demands[item] = (dependency.item_demands[item] + self.item_demands[item]) / 2
